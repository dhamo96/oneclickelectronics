{
    "builds": [{
        "src": "oneclickelectronics/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/electro/static/(.*)",
            "dest": "/oneclickelectronics/electro/static/$1"
        },
        {
            "src": "/inventory/static/(.*)",
            "dest": "/oneclickelectronics/inventory/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "oneclickelectronics/wsgi.py"
        }
    ]
}
